    <mesh name="rightShoulderPitchLink" class="unused" file="../mesh/arms/rightShoulderPitchLink.obj" scale="1 1 1"/>
    <mesh name="rightShoulderRollLink" class="unused" file="../mesh/arms/rightShoulderRollLink.obj" scale="1 1 1"/>
