    <material name="blue" rgba="0 0.1 1 1"/>
    <material name="ctorange" rgba="1.0 0.431 0.1176 1"/>
  </asset>

  <!-- ────────────────────────────────────────────────────────────────────── -->
  <!-- 3. Actuators (ordered to match G1: legs, waist, arms)                 -->
  <!-- ────────────────────────────────────────────────────────────────────── -->
  <actuator>
    <!-- Left Leg -->
    <motor class="hip_pitch" name="left_hip_pitch" joint="left_hip_pitch_joint"/>
